<form id="formTableau">
    <div class="container">
        <div class="row">
            <div class="col-6">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th scope="col">
                                #
                            </th>
                            <th scope="col">
                                email
                            </th>
                            <th scope="col">
                                Ajouter
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <% var compteur =1;%>
                        <% Liste.forEach(function(user){ %>
                        <tr>
                            <th scope="row"><%= compteur %></th>
                            <td><%= user.EMAIL %></td>
                            <td><input type="button" id="<%= user.UTILISATEURID %>" value="Ajouter"></button></td>
                        </tr>
                        <% compteur++; }); %>
                    </tbody>
                </table>
            </div>
            <div class="col-6">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">
                                #
                            </th>
                            <th scope="col">
                                email
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <% var compteur =0;%>
                        <% Liste.forEach(function(user){ %>
                        <tr>
                            <th scope="row"><%= user.UTILISATEURID %></th>
                            <td><%= user.EMAIL %></td>
                        </tr>
                        <% compteur++; }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</form>